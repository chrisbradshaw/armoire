<h1>Your Shoes</h1>

<div class="container">
<ul>
  <% current_user.shoes.each do |i| %>
    <li><%= link_to i.name, i %></li>
  <% end %>
  </ul>
</div>


<%= link_to root_path, class: "btn btn-default" do %>
    <i class="glyphicon glyphicon-arrow-left"></i>
<% end %>

<%= link_to new_shoe_path, class: "btn btn-default" do %>
    <span>Add Shoes!</span>
<% end %>


<%= link_to new_accessory_path, class: "btn btn-default" do %>
    <span>View Accessories!</span>
<% end %>

<%= link_to garments_path, class: "btn btn-default" do %>
    <span>View Garments!</span>
<% end %>
