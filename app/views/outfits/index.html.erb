<div class="container">
  <% Outfit.where(:user_id => current_user.id).limit(1).each do |f|%>
  <div class="row">
    <div class="col-md-4">
      <p><%= f.garment.name%></p>
    </div>
    <div class="col-md-4">
      <p><%= f.shoe.name%></p>
    </div>
    <div class="col-md-4">
      <p><%= f.accessory.name%></p>
    </div>
  </div>
  <%end%>
</div>
